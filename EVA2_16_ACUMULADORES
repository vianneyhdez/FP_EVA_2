package eva2_16_acumuladores;

public class EVA2_16_ACUMULADORES {

    public static void main(String[] args) {
       int acum = 1000;
       System.out.println(acum);
       acum = acum + 200;
       System.out.println(acum);
       acum = acum + 500;
       System.out.println(acum);
       acum = acum - 1200;
       System.out.println(acum);
       acum+=100;//acum = acum + 100;
       System.out.println(acum);
    }
    
}
