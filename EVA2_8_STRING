package eva2_8_string;

public class EVA2_8_STRING {

    public static void main(String[] args) {
        String mensaje = "Hola mundo!!";
        int valor = 100;
        System.out.println(mensaje.charAt(0));
        System.out.println(mensaje.charAt(11));
        char caracter = mensaje.charAt(6);
        System.out.println(caracter);
    }
    
}
